                                @*                 <div class="image"> *@
                                @*                     <a href="#"> *@
                                @*                         <img src="~/theme/images/icon/avatar-1.png" alt="John Doe" /> *@
                                @*                     </a> *@
                                @*                 </div> *@
                                @*                 <div class="content"> *@
                                @*                     <h5 class="name"> *@
                                @*                         <span class="nav-link">Hello, <strong>@User.Identity.Name</strong></span> *@
                                @*                     </h5> *@
                                @*                     <span class="email">@User.Identity.Name</span> *@
                                @*                 </div> *@
                                @*             </div> *@
                                @*             <div class="account-dropdown__body"> *@
                                @*                 <div class="account-dropdown__item"> *@
                                @*                     <a href="#"> *@
                                @*                         <i class="zmdi zmdi-account"></i>Account *@
                                @*                     </a> *@
                                @*                 </div> *@
                                @*                 <div class="account-dropdown__item"> *@
                                @*                     <a href="#"> *@
                                @*                         <i class="zmdi zmdi-settings"></i>Setting *@
                                @*                     </a> *@
                                @*                 </div> *@
                                @*                 <div class="account-dropdown__item"> *@
                                @*                     <a href="#"> *@
                                @*                         <i class="zmdi zmdi-money-box"></i>Billing *@
                                @*                     </a> *@
                                @*                 </div> *@
                                @*             </div> *@
                                @*             <div class="account-dropdown__footer"> *@

                                @*                 @if (User?.Identity?.IsAuthenticated ?? false) *@
                                @*                 { *@
                                @*                     <a class="nav-link" asp-controller="Users" asp-action="Logout"><i class="zmdi zmdi-power"></i>Logout</a> *@

                                @*                 } *@
                                @*                 else *@
                                @*                 { *@
                                @*                     <a class="nav-link" asp-controller="Users" asp-action="Login">Login</a> *@
