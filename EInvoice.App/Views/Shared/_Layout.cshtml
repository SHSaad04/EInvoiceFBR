                                @*                 } *@

                                @*             </div> *@
                                @*         </div> *@
                                @*     </div> *@
                                @* </div> *@
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    @RenderBody()
                    <!-- END MAIN CONTENT-->
                    <!-- END PAGE CONTAINER-->
                </div>
            </div>
        </div>
    </div>


    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2025 - EInvoice.App -
            <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <!-- jQuery should always come first -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap JS (pick one: your theme version OR the lib one, not both) -->
    <script src="~/theme/vendor/bootstrap-5.3.7.bundle.min.js"></script>
    @* If you want to use lib bootstrap instead, comment out the line above and uncomment below *@
    @* <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script> *@

    <!-- Vendor JS -->
    <script src="~/theme/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/theme/vendor/chartjs/Chart.bundle.min.js"></script>

    <!-- Theme / Custom JS -->
    @* <script src="~/theme/js/vanilla-utils.js"></script> *@
    <script src="~/theme/js/bootstrap5-init.js"></script>
    <script src="~/theme/js/main-vanilla.js"></script>
    <script src="~/theme/js/swiper-bundle.min.js"></script>
    <script src="~/theme/js/aos.js"></script>
    <script src="~/theme/js/modern-plugins.js"></script>

    <!-- App-specific JS -->
    <script src="~/js/app.config.js"></script>
    <script src="~/js/app.ajax.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <!-- Validation Scripts -->
    <partial name="_ValidationScriptsPartial" />

    @await RenderSectionAsync("Scripts", required: false)

</body>

</html>
