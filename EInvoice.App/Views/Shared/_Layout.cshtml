                </div>
            </nav>
        </header>
        <!-- END HEADER MOBILE-->
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="#">
                    <img src="~/images/e-incoive-logo.png" alt="E-Invoice" />
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">

                        @if (User?.Identity?.IsAuthenticated ?? false)
                        {
                            <li class="nav-item">
                                <a class="nav-link" asp-controller="Organization" asp-action="Details">Dashboard</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-controller="Organization" asp-action="Details">Organization</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-controller="Client" asp-action="Index">Clients</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-controller="Product" asp-action="Index">Products</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-controller="Invoice" asp-action="Index">Invoices</a>
                            </li>
                        }
                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header" action="" method="POST">
                                <input class="au-input au-input--xl" type="text" name="search" placeholder="Search for datas &amp; reports..." />
                                <button class="au-btn--submit" type="submit">
                                    <i class="zmdi zmdi-search"></i>
                                </button>
                            </form>
                            <div class="header-button">

                            <!-- #region name -->
                                <div class="noti-wrap">
                                    <!-- Messages -->
                                    <div class="noti__item dropdown">
                                        <a class="dropdown-toggle" href="#" role="button" id="dropdownMessages"
                                           data-bs-toggle="dropdown" aria-expanded="false">
                                            <i class="zmdi zmdi-comment-more"></i>
                                            <span class="quantity">1</span>
                                        </a>
                                        <div class="mess-dropdown dropdown-menu" aria-labelledby="dropdownMessages">
                                            <div class="mess__title">
                                                <p>You have 2 news message</p>
                                            </div>
                                            <div class="mess__item">
                                                <div class="image img-cir img-40">
                                                    <img src="images/icon/avatar-06.jpg" alt="Michelle Moreno" />
                                                </div>
                                                <div class="content">
                                                    <h6>Michelle Moreno</h6>
                                                    <p>Have sent a photo</p>
                                                    <span class="time">3 min ago</span>
                                                </div>
                                            </div>
                                            <div class="mess__item">
                                                <div class="image img-cir img-40">
                                                    <img src="images/icon/avatar-04.jpg" alt="Diane Myers" />
                                                </div>
                                                <div class="content">
                                                    <h6>Diane Myers</h6>
                                                    <p>You are now connected on message</p>
                                                    <span class="time">Yesterday</span>
                                                </div>
                                            </div>
                                            <div class="mess__footer">
                                                <a href="#">View all messages</a>
                                            </div>
                                        </div>
                                    </div>

                                    <!-- Emails -->
                                    <div class="noti__item dropdown">
                                        <a class="dropdown-toggle" href="#" role="button" id="dropdownEmails"
                                           data-bs-toggle="dropdown" aria-expanded="false">
                                            <i class="zmdi zmdi-email"></i>
                                            <span class="quantity">1</span>
                                        </a>
                                        <div class="email-dropdown dropdown-menu" aria-labelledby="dropdownEmails">
                                            <div class="email__title">
                                                <p>You have 3 New Emails</p>
                                            </div>
                                            <div class="email__item">
                                                <div class="image img-cir img-40">
                                                    <img src="images/icon/avatar-06.jpg" alt="Cynthia Harvey" />
                                                </div>
                                                <div class="content">
                                                    <p>Meeting about new dashboard...</p>
                                                    <span>Cynthia Harvey, 3 min ago</span>
                                                </div>
                                            </div>
                                            <div class="email__item">
                                                <div class="image img-cir img-40">
                                                    <img src="images/icon/avatar-05.jpg" alt="Cynthia Harvey" />
                                                </div>
                                                <div class="content">
                                                    <p>Meeting about new dashboard...</p>
                                                    <span>Cynthia Harvey, Yesterday</span>
                                                </div>
                                            </div>
                                            <div class="email__item">
                                                <div class="image img-cir img-40">
                                                    <img src="images/icon/avatar-04.jpg" alt="Cynthia Harvey" />
                                                </div>
                                                <div class="content">
                                                    <p>Meeting about new dashboard...</p>
                                                    <span>Cynthia Harvey, January 15, 2025</span>
                                                </div>
                                            </div>
                                            <div class="email__footer">
                                                <a href="#">See all emails</a>
                                            </div>
                                        </div>
                                    </div>

                                    <!-- Notifications -->
                                    <div class="noti__item dropdown">
                                        <a class="dropdown-toggle" href="#" role="button" id="dropdownNotifs"
                                           data-bs-toggle="dropdown" aria-expanded="false">
                                            <i class="zmdi zmdi-notifications"></i>
                                            <span class="quantity">3</span>
                                        </a>
                                        <div class="notifi-dropdown dropdown-menu" aria-labelledby="dropdownNotifs">
                                            <div class="notifi__title">
                                                <p>You have 3 Notifications</p>
                                            </div>
                                            <div class="notifi__item">
                                                <div class="bg-c1 img-cir img-40">
                                                    <i class="zmdi zmdi-email-open"></i>
                                                </div>
                                                <div class="content">
                                                    <p>You got a email notification</p>
                                                    <span class="date">January 15, 2025 14:30</span>
                                                </div>
                                            </div>
                                            <div class="notifi__item">
                                                <div class="bg-c2 img-cir img-40">
                                                    <i class="zmdi zmdi-account-box"></i>
                                                </div>
                                                <div class="content">
                                                    <p>Your account has been blocked</p>
                                                    <span class="date">January 15, 2025 14:30</span>
                                                </div>
                                            </div>
                                            <div class="notifi__item">
                                                <div class="bg-c3 img-cir img-40">
                                                    <i class="zmdi zmdi-file-text"></i>
                                                </div>
                                                <div class="content">
                                                    <p>You got a new file</p>
                                                    <span class="date">January 15, 2025 14:30</span>
                                                </div>
                                            </div>
                                            <div class="notifi__footer">
                                                <a href="#">All notifications</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            <!-- #endregion -->
                                <div class="account-wrap">
                                    <div class="account-item clearfix dropdown">
                                        <a href="#" class="d-flex align-items-center dropdown-toggle" id="accountDropdown"
                                           role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                            <div class="image">
                                                <img src="~/theme/images/icon/avatar-1.png" alt="John Doe" />
                                            </div>
                                            <div class="content ms-2">
                                                @if (User?.Identity?.IsAuthenticated ?? false)
                                                {
                                                    <span class="nav-link">Hello, <strong>@User.Identity.Name</strong></span>
                                                }
                                            </div>
                                        </a>

                                        <div class="account-dropdown dropdown-menu" aria-labelledby="accountDropdown">
                                            <div class="info clearfix px-3">
                                                <div class="image">
                                                    <a href="#">
                                                        <img src="~/theme/images/icon/avatar-1.png" alt="John Doe" />
                                                    </a>
                                                </div>
                                                <div class="content ms-2">
                                                    <h5 class="name">
                                                        <span class="nav-link">Hello, <strong>@User.Identity.Name</strong></span>
                                                    </h5>
                                                    <span class="email">@User.Identity.Name</span>
                                                </div>
                                            </div>

                                            <div class="account-dropdown__body mt-3">
                                                <div class="account-dropdown__item">
                                                    <a href="#">
                                                        <i class="zmdi zmdi-account"></i>Account
                                                    </a>
                                                </div>
                                                <div class="account-dropdown__item">
                                                    <a href="#">
                                                        <i class="zmdi zmdi-settings"></i>Setting
                                                    </a>
                                                </div>
                                                <div class="account-dropdown__item">
                                                    <a href="#">
                                                        <i class="zmdi zmdi-money-box"></i>Billing
                                                    </a>
                                                </div>
                                            </div>

                                            <div class="account-dropdown__footer mt-3">
                                                @if (User?.Identity?.IsAuthenticated ?? false)
                                                {
                                                    <a class="nav-link" asp-controller="Users" asp-action="Logout">
                                                        <i class="zmdi zmdi-power"></i>Logout
                                                    </a>
                                                }
                                                else
                                                {
                                                    <a class="nav-link" asp-controller="Users" asp-action="Login">Login</a>
                                                    <a class="nav-link" asp-controller="Users" asp-action="Register">Register</a>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>

